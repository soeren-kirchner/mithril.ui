{
  "name": "mithril.ui",
  "version": "0.1.0",
  "description": "mithril.ui is a kind of extension to the modern and sophisticated [Mithril](https://mithril.js.org) Framework.",
  "main": "global.js",
  "directories": {},
  "devDependencies": {
    "esbuild": "0.14.27",
    "npm-run-all": "4.1.5"
  },
  "scripts": {
    "build:esm": "esbuild src/lib.js --bundle --outfile=dist/mithril.ui.esm.js --format=esm",
    "build:cjs": "esbuild src/lib.js --bundle --outfile=dist/mithril.ui.cjs.js --format=cjs",
    "build:global": "esbuild src/global.js --bundle --minify --outfile=dist/mithril.ui.global.js",
    "build": "run-p build:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soeren-kirchner/mithril.ui.git"
  },
  "keywords": [
    "mithril",
    "ui"
  ],
  "author": "sk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soeren-kirchner/mithril.ui/issues"
  },
  "homepage": "https://github.com/soeren-kirchner/mithril.ui#readme",
  "dependencies": {
  }
}
